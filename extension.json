{
	"name": "CommunityConfigurationExample",
	"author": [
		"Sergio Gimeno"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:TBD",
	"descriptionmsg": "communityconfigurationexample-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.42.0"
	},
	"attributes": {
		"CommunityConfiguration": {
			"Providers": {
				"FooBar": {
					"store": {
						"type": "wikipage",
						"args": [
							"MediaWiki:ExampleConfig.json"
						]
					},
					"validator": {
						"type": "jsonschema",
						"args": [
							"CommunityConfigurationExample\\Schemas\\ExampleSchema"
						]
					},
					"type": "mw-config"
				}
			}
		}
	},
	"AutoloadNamespaces": {
		"CommunityConfigurationExample\\": "src/"
	},
	"config": {
	},
	"HookHandlers": {
	},
	"Hooks": {
	},
	"MessagesDirs": {
		"CommunityConfigurationExample": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"CommunityConfigurationExampleAlias": "CommunityConfigurationExample.alias.php"
	},
	"ResourceModules": {
		"ext.boilerPlate": {
			"localBasePath": "resources/ext.boilerPlate",
			"remoteExtPath": "BoilerPlate/resources/ext.boilerPlate",
			"dependencies": [],
			"styles": [],
			"packageFiles": [
				"init.js"
			],
			"messages": []
		}
	},
	"SpecialPages": {
		"CommunityConfigurationExample": {
			"class": "CommunityConfigurationExample\\Specials\\SpecialCommunityConfigurationExample",
			"services": [
				"CommunityConfiguration.MediaWikiConfigReader"
			]
		}
	},
	"manifest_version": 2
}
