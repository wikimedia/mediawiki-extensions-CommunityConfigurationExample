{
	"name": "CommunityConfigurationExample",
	"author": [
		"Sergio Gimeno"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:TBD",
	"descriptionmsg": "communityconfigurationexample-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.42.0"
	},
	"attributes": {
		"CommunityConfiguration": {
			"Providers": {
				"FooBar": {
					"store": {
						"type": "wikipage",
						"args": [
							"MediaWiki:ExampleConfig.json"
						]
					},
					"validator": {
						"type": "jsonschema",
						"args": [
							"CommunityConfigurationExample\\Schemas\\ExampleSchema"
						]
					},
					"type": "mw-config"
				},
				"Mentorship": {
					"store": {
						"type": "wikipage",
						"args": [
							"MediaWiki:GrowthExperimentsMentorship.json"
						]
					},
					"validator": {
						"type": "jsonschema",
						"args": [
							"CommunityConfigurationExample\\Schemas\\GrowthMentorshipSchema"
						]
					},
					"form": {
						"type": "jsonform",
						"args": [
							"ext.communityConfiguration.GrowthExperiments"
						]
					},
					"type": "mw-config"
				}
			}
		}
	},
	"AutoloadNamespaces": {
		"CommunityConfigurationExample\\": "src/"
	},
	"config": {
	},
	"HookHandlers": {
	},
	"Hooks": {
	},
	"MessagesDirs": {
		"CommunityConfigurationExample": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"CommunityConfigurationExampleAlias": "CommunityConfigurationExample.alias.php"
	},
	"ResourceModules": {
		"ext.communityConfiguration.GrowthExperiments": {
			"localBasePath": "resources/ext.communityConfiguration.GrowthExperiments",
			"remoteExtPath": "BoilerPlate/resources/ext.communityConfiguration.GrowthExperiments",
			"dependencies": [
				"vue",
				"@wikimedia/codex",
				"ext.communityConfiguration.Editor"
			],
			"styles": [],
			"packageFiles": [
				"index.js",
				"BooleanYesNoControl.vue"
			],
			"messages": [
				"communityconfiguration-growthexperiments-custom-boolean-control-yes",
				"communityconfiguration-growthexperiments-custom-boolean-control-no"
			]
		}
	},
	"SpecialPages": {
		"CommunityConfigurationExample": {
			"class": "CommunityConfigurationExample\\Specials\\SpecialCommunityConfigurationExample",
			"services": [
				"CommunityConfigurationExample.WikiPageConfig"
			]
		}
	},
	"ServiceWiringFiles": [
		"src/ServiceWiring.php"
	],
	"manifest_version": 2
}
